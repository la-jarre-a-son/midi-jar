{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "chordDisplay": {
            "items": {
                "properties": {
                    "allowOmissions": {
                        "type": "boolean"
                    },
                    "chordNotation": {
                        "enum": [
                            "long",
                            "short",
                            "symbol"
                        ],
                        "type": "string"
                    },
                    "displayAltChords": {
                        "type": "boolean"
                    },
                    "displayChord": {
                        "type": "boolean"
                    },
                    "displayIntervals": {
                        "type": "boolean"
                    },
                    "displayKeyboard": {
                        "type": "boolean"
                    },
                    "displayName": {
                        "type": "boolean"
                    },
                    "displayNotation": {
                        "type": "boolean"
                    },
                    "highlightAlterations": {
                        "type": "boolean"
                    },
                    "id": {
                        "type": "string"
                    },
                    "keyboard": {
                        "properties": {
                            "colors": {
                                "properties": {
                                    "black": {
                                        "type": [
                                            "null",
                                            "string"
                                        ]
                                    },
                                    "played": {
                                        "type": [
                                            "null",
                                            "string"
                                        ]
                                    },
                                    "sustained": {
                                        "type": [
                                            "null",
                                            "string"
                                        ]
                                    },
                                    "white": {
                                        "type": [
                                            "null",
                                            "string"
                                        ]
                                    },
                                    "wrapped": {
                                        "type": [
                                            "null",
                                            "string"
                                        ]
                                    }
                                },
                                "type": "object"
                            },
                            "displaySustained": {
                                "type": "boolean"
                            },
                            "fadeOutDuration": {
                                "type": "number"
                            },
                            "from": {
                                "type": "string"
                            },
                            "keyInfo": {
                                "enum": [
                                    "interval",
                                    "none",
                                    "tonic",
                                    "tonicAndInterval"
                                ],
                                "type": "string"
                            },
                            "keyName": {
                                "enum": [
                                    "none",
                                    "note",
                                    "octave",
                                    "pitchClass"
                                ],
                                "type": "string"
                            },
                            "label": {
                                "enum": [
                                    "chordNote",
                                    "interval",
                                    "none",
                                    "note",
                                    "pitchClass"
                                ],
                                "type": "string"
                            },
                            "sizes": {
                                "properties": {
                                    "bevel": {
                                        "type": "boolean"
                                    },
                                    "height": {
                                        "type": "number"
                                    },
                                    "radius": {
                                        "type": "number"
                                    },
                                    "ratio": {
                                        "type": "number"
                                    }
                                },
                                "type": "object"
                            },
                            "skin": {
                                "enum": [
                                    "classic",
                                    "flat"
                                ],
                                "type": "string"
                            },
                            "textOpacity": {
                                "type": "number"
                            },
                            "to": {
                                "type": "string"
                            },
                            "wrap": {
                                "type": "boolean"
                            }
                        },
                        "type": "object"
                    },
                    "useSustain": {
                        "type": "boolean"
                    }
                },
                "type": "object"
            },
            "type": "array"
        },
        "chordQuiz": {
            "properties": {
                "chordNotation": {
                    "enum": [
                        "long",
                        "short",
                        "symbol"
                    ],
                    "type": "string"
                },
                "difficulty": {
                    "enum": [
                        0,
                        1,
                        2,
                        3,
                        4,
                        5
                    ],
                    "type": "number"
                },
                "displayIntervals": {
                    "type": "boolean"
                },
                "displayName": {
                    "type": "boolean"
                },
                "displayReaction": {
                    "type": "boolean"
                },
                "gameLength": {
                    "type": "number"
                },
                "gamification": {
                    "type": "boolean"
                },
                "mode": {
                    "enum": [
                        "random",
                        "randomInKey"
                    ],
                    "type": "string"
                }
            },
            "type": "object"
        },
        "circleOfFifths": {
            "properties": {
                "displayAlterations": {
                    "type": "boolean"
                },
                "displayDegreeLabels": {
                    "type": "boolean"
                },
                "displayDegrees": {
                    "type": "boolean"
                },
                "displayDiminished": {
                    "type": "boolean"
                },
                "displayDominants": {
                    "type": "boolean"
                },
                "displayMajor": {
                    "type": "boolean"
                },
                "displayMinor": {
                    "type": "boolean"
                },
                "displayModes": {
                    "type": "boolean"
                },
                "displaySuspended": {
                    "type": "boolean"
                },
                "highlightInScale": {
                    "type": "boolean"
                },
                "highlightSector": {
                    "enum": [
                        "chord",
                        "notes"
                    ],
                    "type": "string"
                },
                "scale": {
                    "enum": [
                        "major",
                        "minor"
                    ],
                    "type": "string"
                }
            },
            "type": "object"
        },
        "general": {
            "properties": {
                "launchAtStartup": {
                    "type": "boolean"
                },
                "startMinimized": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "notation": {
            "properties": {
                "accidentals": {
                    "enum": [
                        "flat",
                        "sharp"
                    ],
                    "type": "string"
                },
                "key": {
                    "type": "string"
                },
                "staffClef": {
                    "enum": [
                        "bass",
                        "both",
                        "treble"
                    ],
                    "type": "string"
                },
                "staffTranspose": {
                    "type": "number"
                }
            },
            "type": "object"
        },
        "server": {
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "port": {
                    "type": "number"
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}

